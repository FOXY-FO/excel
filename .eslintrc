{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "rules": {
        "semi": [2, "never"],
        "arrow-parens": "off",
        "require-jsdoc": "off"
    }
}